{
    "name": "Video Speed Up",
    "description": "A extenstion for Video to accel speed",
    "version": "1.3",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "minimum_chrome_version": "16.0.884",
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["content.js"]
        }],
    "permissions": ["*://*/*","storage","activeTab","tabs"],
    "browser_action": {"default_popup": "popup.html"},//{"name":"Click to start"},
    "manifest_version": 2
}
