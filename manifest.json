{
    "name": "Video Speed Up",
    "description": "A extenstion for Video to accel speed. Change show-up speed color by button. Shortkeys: press [w] Show/Fade speed, press [a] Speed up, press [s] Reset speed, press [d] Speed down",
    "version": "2.0",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "minimum_chrome_version": "16.0.884",
    "content_scripts":[{
        "matches":["http://*/*","https://*/*"],
        "js":["content.js"]
        }],
    "permissions": ["*://*/*","storage","activeTab","tabs"],
    "browser_action": {"default_popup": "popup.html"},
    "icons":{
        "153":"VideoSpeedUp_Transparent.png"
    },
    "manifest_version": 2
}
